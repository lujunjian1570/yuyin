
























/*每个页面公共css */
page,view{
	box-sizing: border-box;
}
page{
	height: 100%;
}
image{
	will-change: transform
}
.line{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: block;
}
.line2{
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}

